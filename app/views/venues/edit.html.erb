<h2> Edit Venue Info </h2>

<%= form_for @venue, :html => { :multipart => true } do |f| %>
  <div class="field">
    <%= f.label :avatar %><br />
    <%= f.file_field :avatar %><br /><br />
  </div>

  <div class="field">
  <%= f.label :venue_title %><br />
  <%= f.text_field :venue_title %><br /><br />
  </div>

  <div class="field">
  <%= f.label :venue_describe %><br />
  <%= f.text_area :venue_describe %><br /><br />

  <div class="field">
  <%= f.label :contact %><br />
  <%= f.text_field :contact %><br /><br />
  <%= f.hidden_field :venue_id, value: @venue.id %>

  <%= f.submit %>
<% end %>